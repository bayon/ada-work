                                            html_markup.Append("<img src='").Append(Core.GetImageBaseUrl("ART")).Append(home_slideshow_image3[i]).Append("' alt='").Append(home_slideshow_title3[i]).Append("' />");
                                        }

                                        if (home_slideshow_link3[i].Trim() != "" && home_slideshow_link3[i].Trim() != "#") {
                                            html_markup.Append("</a>");
                                        }
                                    html_markup.Append("</div>");
                                }

                                //Slide section 4
                                if (home_slideshow_image4[i] != ""){
                                    html_markup.Append("<div style=\"padding:0px !important; margin:0px !important;\" class=\"").Append(home_slideshow_class).Append("\">");
                                        if (home_slideshow_link4[i].Trim() != "" && home_slideshow_link4[i].Trim() != "#") {
                                            html_markup.Append("<a href=\"").Append(home_slideshow_link4[i]).Append("\" title=\"").Append(home_slideshow_title4[i]).Append("\">");
                                        }

                                        if (home_slideshow_mobile_image[i].Trim() != "") {
                                            html_markup.Append("<img class='hidden-xs' src='").Append(Core.GetImageBaseUrl("ART")).Append(home_slideshow_image4[i]).Append("' alt='").Append(home_slideshow_title4[i]).Append("' />");
                                        }else {
                                            html_markup.Append("<img src='").Append(Core.GetImageBaseUrl("ART")).Append(home_slideshow_image4[i]).Append("' alt='").Append(home_slideshow_title4[i]).Append("' />");
                                        }

                                        if (home_slideshow_link4[i].Trim() != "" && home_slideshow_link4[i].Trim() != "#") {
                                            html_markup.Append("</a>");
                                        }
                                    html_markup.Append("</div>");
                                }


                            html_markup.Append("</li>");


                            //Video Block (Hard Coded): START
                            if (i+1 == show_video_after_slide_number && show_video_slide == true) {
                                html_markup.Append("<li>");
                                    html_markup.Append("<a class=\"dialog iframe\" title=\"").Append(video_slide_title).Append("\" href=\"https://www.youtube.com/embed/").Append(video_youtube_code).Append("?autoplay=1&fs=1&rel=0\">");
                                        
                                    html_markup.Append("</a>");
                                html_markup.Append("</li>");
                            }
                            //Video Block (Hard Coded): END

                        }
                    }
                html_markup.Append("</ul>");
            html_markup.Append("</div>");

            //Home Featured Promos
            int home_featured_counter = 0;
            for (int c = 0; c < home_featured_image.Count; c++) {
                if (home_featured_image[c].Trim() != ""){
                    home_featured_counter++;
                }
            }

            String home_featured_column_class = "col-xs-12 col-sm-12 col-md-12 col-lg-12";
            switch (home_featured_counter) {
                case 1:
                    home_featured_column_class = "col-xs-12 col-sm-12 col-md-12 col-lg-12";
                    break;
                case 2:
                    home_featured_column_class = "col-xs-12 col-sm-6 col-md-6 col-lg-6";
                    break;
                case 3:
                    home_featured_column_class = "col-xs-12 col-sm-4 col-md-4 col-lg-4";
                    break;
                case 4:
                    home_featured_column_class = "col-xs-12 col-sm-6 col-md-3 col-lg-3";
                    break;
                default:
                    home_featured_column_class = "col-xs-12 col-sm-12 col-md-12 col-lg-12";
                    break;
            }

            html_markup.Append("<div id=\"home_featured_promos\" class=\"container-fluid\" style=\"padding-top:25px !important;\">");
                for (int i = 0; i < home_featured_image.Count; i++) {
                    if (home_featured_image[i] == ""){
                        break;
                    }else {
                        html_markup.Append("<div class=\"").Append(home_featured_column_class).Append("\">");
                            html_markup.Append("<a style=\"display:block;\"").Append(" href=\"").Append(home_featured_link[i]).Append("\">");
                                html_markup.Append("<div style=\"padding: 12px;text-align: center;font-size: 18px;border: 1px solid #cecece;color: #3e3e3e;\">").Append(home_featured_title[i]).Append("</div>");
                                html_markup.Append("<img style=\"width:100%;\" src=\"").Append(Core.GetImageBaseUrl("ROOT")).Append(home_featured_image[i]).Append("\" alt=\"").Append(home_featured_title[i]).Append(" promo\" />");
                            html_markup.Append("</a>");
                        html_markup.Append("</div>");
                    }
                }
            html_markup.Append("</div>");

        }
    html_markup.Append("</div>");

    return html_markup.ToString();
}
</script>
